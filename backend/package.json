    "dotenv": "^14.3.2",
    "escape-html": "^1.0.3",
    "express": "^4.17.2",
    "express-csp-header": "^5.0.0",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "http-terminator": "^3.0.4",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.8",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-twitch-new": "^0.0.2",
