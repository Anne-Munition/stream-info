{
  "name": "streaminfo-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r dotenv/config src",
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json",
    "prettier": "prettier --write \"**/*.{js,ts,md,*rc,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts --ignore-path ../.gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^14.3.2",
    "escape-html": "^1.0.3",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "http-terminator": "^3.0.4",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "mongoose": "^6.1.8",
    "morgan": "^1.10.0",
    "winston": "^3.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/escape-html": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
